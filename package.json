{
  "name": "battles",
  "version": "0.0.1",
  "description": "Roguelike game situated in cold north",
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "blanket": "^1.2.3",
    "command-line-args": "^5.0.2",
    "eslint": "^4.19.1",
    "file-saver": "^1.3.8",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-notify": "^3.0.0",
    "gulp-sass": "^3.2.1",
    "js-md5": "^0.6.0",
    "localforage": "^1.7.2",
    "marked": "^0.3.19",
    "prettybnf": "^0.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-bootstrap": "^0.31.3",
    "react-bootstrap-modal": "^4.1.1",
    "react-contextmenu": "^2.9.3",
    "react-dom": "^16.5.0",
    "react-modal": "^3.5.1",
    "react-test-renderer": "^16.5.0",
    "sass": "^1.13.2",
    "uglify-es": "^3.0.25",
    "vinyl-source-stream": "^1.1.0",
    "webworkify": "^1.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-incremental": "^3.1.1",
    "chai": "~3.5.0",
    "chai-enzyme": "^0.8.0",
    "coveralls": "^2.12.0",
    "enzyme": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint_d": "^5.3.1",
    "fake-indexeddb": "^2.0.3",
    "gulp-rename": "^1.4.0",
    "istanbul": "~0.4.5",
    "jsdom": "^11.12.0",
    "memwatch-next": "^0.3.0",
    "mocha": "~3.1",
    "node-localstorage": "^1.3.1",
    "nyc": "^11.9.0",
    "sass-lint": "^1.10.2"
  },
  "engines": {
    "node": ">= 7.0"
  },
  "contributors": [
    "Tuomas Poikela <tuomas.sakari.poikela@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/tpoikela/battles.git"
  },
  "nyc": {
    "exclude": [
      "**/rot.js",
      "tests"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "scripts": {
    "build": "npm run build:sass && npm run build:production",
    "build:production": "bin/build_production.sh",
    "build:production:debug": "npm run create-config-prod && NODE_ENV=production ./browserify --debug ./client/jsx/app.jsx --extension .jsx -t babelify > ./build/bundle.js",
    "build:sass": "sass scss/style.scss --style compressed > build/style.css",
    "clean": "bin/clean.sh",
    "create-config-prod": "cp -n client/config/production.js public/config.js",
    "create-config-devel": "cp -n client/config/devel.js public/config.js",
    "coverage": "perl bin/cov_test.pl",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "hooks": "perl bin/deploy_hooks",
    "start": "python -m SimpleHTTPServer",
    "test": "mocha --compilers js:babel-core/register --recursive tests/client/src",
    "test:functional": "mocha --compilers js:babel-core/register tests/client/functional/*.js",
    "test:gui": "mocha --compilers js:babel-core/register --recursive tests/client/gui",
    "test:jsx": "NODE_ENV=test mocha --require tests/helpers/browser.js --compilers .:babel-core/register tests/client/jsx/*.jsx",
    "test:all": "npm run test && npm run test:gui && npm run test:functional && npm run test:jsx"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpoikela/battles/issues"
  }
}
