{
  "name": "battles",
  "version": "0.0.1",
  "description": "Roguelike game situated in cold north",
  "main": "main.electron.js",
  "dependencies": {
    "@types/node": "^10.12.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "blanket": "^1.2.3",
    "command-line-args": "^5.0.2",
    "csstype": "^2.5.7",
    "eslint": "^4.19.1",
    "file-loader": "^2.0.0",
    "file-saver": "^1.3.8",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-notify": "^3.0.0",
    "gulp-sass": "^3.2.1",
    "js-md5": "^0.6.0",
    "linewrap": "^0.2.1",
    "localforage": "^1.7.2",
    "marked": "^0.3.19",
    "prettybnf": "^0.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-bootstrap": "^0.31.3",
    "react-bootstrap-modal": "^4.1.1",
    "react-contextmenu": "^2.9.3",
    "react-dom": "^16.5.0",
    "react-modal": "^3.5.1",
    "react-test-renderer": "^16.5.0",
    "sass": "^1.13.2",
    "uglify-es": "^3.0.25",
    "vinyl-source-stream": "^1.1.0",
    "webworkify": "^1.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "browserify": "^16.2.0",
    "browserify-incremental": "^3.1.1",
    "chai": "~3.5.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "coveralls": "^2.12.0",
    "electron": "^3.0.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint_d": "^5.3.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fake-indexeddb": "^2.0.3",
    "gulp-rename": "^1.4.0",
    "html-loader": "^0.5.5",
    "istanbul": "~0.4.5",
    "jsdom": "~12.0.0",
    "json-loader": "^0.5.7",
    "markdown-loader": "^3.0.0",
    "memwatch-next": "^0.3.0",
    "mocha": "~3.1",
    "node-localstorage": "^1.3.1",
    "node-sass": "^4.9.0",
    "nyc": "^11.9.0",
    "raw-loader": "^0.5.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-consistent-codestyle": "^1.14.1",
    "typescript": "^3.1.6",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "uglifyify": "^5.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-devserver": "0.0.6"
  },
  "engines": {
    "node": ">= 7.0"
  },
  "contributors": [
    "Tuomas Poikela <tuomas.sakari.poikela@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/tpoikela/battles.git"
  },
  "nyc": {
    "exclude": [
      "**/rot.js",
      "tests"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "scripts": {
    "build": "npm run build:sass && npm run build:production",
    "build:production": "bin/build_production.sh",
    "build:production:debug": "npm run create-config-prod && NODE_ENV=production ./browserify --debug ./client/jsx/app.jsx --extension .jsx -t babelify -t uglifyify | uglifyjs --mangle --compress --source-map -o ./build/bundle.js",
    "build:inc": "npm run create-config-prod && NODE_ENV=production browserifyinc --cachefile br-cache-prod.json --debug ./client/jsx/app.jsx --extension .jsx -t babelify -t uglifyify | uglifyjs --mangle --compress --source-map -o ./build/bundle.js",
    "build:sass": "sass scss/style.scss --style compressed > build/style.css",
    "clean": "bin/clean.sh",
    "create-config-prod": "cp -n client/config/production.js public/config.js",
    "create-config-devel": "cp -n client/config/devel.js public/config.js",
    "coverage": "perl bin/cov_test.pl",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "electron": "electron .",
    "hooks": "perl bin/deploy_hooks",
    "start": "python -m SimpleHTTPServer",
    "test": "mocha --compilers js:babel-core/register --recursive tests/client/src",
    "test:ts": "mocha -r ts-node/register 'tests/client/**/*.spec.ts'",
    "test:tsx": "mocha -r ts-node/register 'tests/client/**/*.spec.tsx'",
    "test:functional": "mocha --compilers js:babel-core/register tests/client/functional/*.js",
    "test:gui": "mocha --compilers js:babel-core/register --recursive tests/client/gui",
    "test:jsx": "NODE_ENV=test mocha --require tests/helpers/browser.js --compilers .:babel-core/register tests/client/jsx/*.jsx",
    "test:all": "npm run test && npm run test:gui && npm run test:functional && npm run test:jsx"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tpoikela/battles/issues"
  }
}
